<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="7897d6ca-b536-4837-b1b2-007a25467ece" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2020.1/project-config.xsd">
  <name>conf2</name>
  <description />
  <settings>
    <options>
      <option name="cleanBuild" value="true" />
    </options>
    <disabled-settings>
      <setting-ref ref="BUILD_EXT_2" />
      <setting-ref ref="BUILD_EXT_3" />
      <setting-ref ref="BUILD_EXT_4" />
      <setting-ref ref="RUNNER_1" />
      <setting-ref ref="jetbrains.agent.free.space" />
    </disabled-settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_2" name="" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[sleep 80
mkdir dir40
for i in $(seq 1 1 39)
do
	echo "A" > dir40/$i
done
echo "##teamcity[publishArtifacts '+:**/dir40/* => dir40']"
echo "step1 part1"
mkdir dir50
for i in $(seq 40 1 89)
do
	touch dir50/$i
done
echo "##teamcity[publishArtifacts '+:**/dir50/* => dir50']"
echo "step 1 part2"]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_1" name="New build step" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[echo "##teamcity[publishArtifacts '+:**/dir40/* => dir40']"
echo "step2"]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ArtifactsMax_SampleProj" />
      <vcs-entry-ref root-id="ArtifactsMax_ChubatovaVcs" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="jetbrains.agent.free.space" type="jetbrains.agent.free.space">
        <parameters>
          <param name="free-space-work" value="3000000tb" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="BuildFailureOnMetric">
        <parameters>
          <param name="metricKey" value="VisibleArtifactsSize" />
          <param name="metricThreshold" value="1 mb" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_3" type="BuildFailureOnMessage">
        <parameters>
          <param name="buildFailureOnMessage.conditionType" value="contains" />
          <param name="buildFailureOnMessage.messagePattern" value="chubatova" />
          <param name="buildFailureOnMessage.outputText" value=".привет от штиблет!)_)(№У&quot;№ЪХЗ." />
          <param name="buildFailureOnMessage.reverse" value="false" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_4" type="BuildFailureOnMetric">
        <parameters>
          <param name="metricKey" value="VisibleArtifactsSize" />
          <param name="metricThreshold" value="500 mb" />
          <param name="metricUnits" value="metricUnitsDefault" />
          <param name="moreOrLess" value="more" />
          <param name="withBuildAnchor" value="false" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

